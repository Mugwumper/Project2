<li>
    <div class="row">
        <h3>Entries with tag: {{name}}</h3>
    </div>

    {{#each journals}}
    <div class="row">
        <div class="col s10 entryDescription">
            {{description}}
        </div>
        <div class="col s2">
            {{!-- <button class="edit-entry btn-small" data-id="{{this.id}}">EDIT/UPDATE</button> --}}
            <button class="delete-entry btn-small" data-id="{{this.id}}"><i class="material-icons">cancel</i></button>
        </div>
    </div>
    {{/each}}

</li>