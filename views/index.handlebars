<div id="home">
  <div class="container">
    <div class="row">
      <div class="col s8 offset-2">
        <h1>{{msg}}</h1>
        <h2>Dream Journal</h2>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="journal-title">Title</label>
            <input type="text" id="journal-title" class="form-control" aria-describedby="journal-title"
              placeholder="Enter Dream Title (optional)" />
          </div>
          <div class="input-field col s12">
            <label for="journal-description">Dream Description</label>
            <textarea class="form-control" id="journal-description" placeholder="Enter Dream Description"></textarea>
          </div>
          <div class="form-group">
            <label for="tags">Tags</label>
            <textarea class="form-control" id="tags" aria-describedby="tags" placeholder="Enter Tag"></textarea>
          </div>
          <button id="submit" class="btn btn-primary float-right">Submit</button>
        </form>
        <h3>Journal Entries:</h3>
        <ul id="journal-list" class="list-group">
          {{#each examples}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="jounral/{{this.id}}">{{this.text}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>